a = repmat(1:1000, 1000,1); imagesc(a);
colors = colormap(hsv);
colors(48:end,:) = [];
k=[ .65 .18 ]; col = k(1)*colors+k(2); 

%%
a = repmat(1:1000, 1000,1); figure; imagesc(a);
set(gcf,'colormap', col);

